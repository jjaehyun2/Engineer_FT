{
    "quality_thresholds": {
        "python": 0.6,
        "javascript": 0.55,
        "java": 0.6,
        "default": 0.5
    },
    "file_size_limits": {
        "min_bytes": 200,
        "max_bytes": 500000,
        "min_lines": 10,
        "max_lines": 1000
    },
    "docstring_requirements": {
        "python": {
            "require_module_docstring": false,
            "require_function_docstrings": true,
            "min_docstring_functions_ratio": 0.3
        },
        "javascript": {
            "require_file_header": false,
            "require_function_docstrings": false,
            "min_docstring_functions_ratio": 0.1
        }
    },
    "complexity_limits": {
        "max_cyclomatic_complexity": 20,
        "max_function_length": 100
    },
    "domain_balancing": {
        "enabled": true,
        "target_distribution": {
            "algorithms": 0.2,
            "web": 0.2,
            "data": 0.2,
            "system": 0.2,
            "ml": 0.1,
            "general": 0.1
        }
    },
    "deduplication": {
        "enabled": true,
        "method": "content_hash",
        "similarity_threshold": 0.9
    },
    "max_files_per_repo": 1000
}